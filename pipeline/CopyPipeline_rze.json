{
	"name": "CopyPipeline_rze",
	"properties": {
		"activities": [
			{
				"name": "ForEach_rze",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_rze",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "@{item().source.collection}"
								},
								{
									"name": "Destination",
									"value": "dbo.@{item().destination.table}"
								}
							],
							"typeProperties": {
								"enableStaging": false,
								"enableSkipIncompatibleRow": true
							}
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"collection": "questions"
						},
						"destination": {
							"table": "questions"
						},
						"copyActivity": {
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"path": "$['_id']"
										},
										"sink": {
											"name": "ID"
										}
									},
									{
										"source": {
											"path": "$['question'][0]['text']"
										},
										"sink": {
											"name": "QUESTION TEXT"
										}
									},
									{
										"source": {
											"path": "$['question'][0]['type']"
										},
										"sink": {
											"name": "TYPE"
										}
									},
									{
										"source": {
											"path": "$['question'][0]['options'][0]"
										},
										"sink": {
											"name": "OPTIONS"
										}
									},
									{
										"source": {
											"path": "$['surveyId']"
										},
										"sink": {
											"name": "SURVEY ID"
										}
									}
								]
							}
						}
					},
					{
						"source": {
							"collection": "responses"
						},
						"destination": {
							"table": "responses"
						},
						"copyActivity": {
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"path": "$['_id']"
										},
										"sink": {
											"name": "ID"
										}
									},
									{
										"source": {
											"path": "$['respondentId']"
										},
										"sink": {
											"name": "RESPONDENT ID"
										}
									},
									{
										"source": {
											"path": "$['surveyId']"
										},
										"sink": {
											"name": "SURVEY ID"
										}
									},
									{
										"source": {
											"path": "$['date']['$date']['$numberLong']"
										},
										"sink": {
											"name": "DATE"
										}
									},
									{
										"source": {
											"path": "$['answers'][0]"
										},
										"sink": {
											"name": "ANSWERS"
										}
									}
								]
							}
						}
					},
					{
						"source": {
							"collection": "surveys"
						},
						"destination": {
							"table": "surveys"
						},
						"copyActivity": {
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"path": "$['_id']"
										},
										"sink": {
											"name": "ID"
										}
									},
									{
										"source": {
											"path": "$['name']"
										},
										"sink": {
											"name": "NAME"
										}
